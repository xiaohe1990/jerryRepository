body {
	overflow:hidden;
}
.fe-login {
	position:absolute;
	top:180px;right:240px;
	width:380px;
	z-index:9999;
	border-radius:5px;
	background: rgba(225,225,225,0.3);
}
.panel.fe-login {
	border:none;
}
.fe-login .input-group + .input-group {
	margin-top:15px;
}
.login_btn{padding: 6px 156px;}
.login_head{ background:#fff; }

.fe-login .panel-footer_a {
	margin:0 3px;
}
.fe-login .input-group_btnBox {
	width:100%;
}
.fe-login_lgType {
	display:inline-block;
	border: 1px solid #eee;
    border-radius: 50%;
    overflow: hidden;
    width: 82px;
    height: 82px;
    line-height:82px;
    margin:15px;
    font-size:32px;
}
.fe-login_lgType:hover {
	background-color:#fdd31c;
	border: 1px solid #fdd31c;
}
.fe-login_canvasBg {
	width: 100%;
    background:url('../../images/sys/loginBg.jpg') no-repeat;
    background-size:100% 100%;
    height:640px;
    position:absolute;
    z-index:9998;
    filter:alpha(opacity=40);
    -moz-opacity:0.4;
    -khtml-opacity:0.4;
    opacity:0.4;
}