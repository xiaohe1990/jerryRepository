package com.mall.goods.model;
/**
 * 
* @ClassName: GAttributeOption 
* @Description: TODO(商品属性选项实体) 
* @author XW 
* @date 2018年1月11日 下午9:12:03 
*
 */
public class GAttributeOption {
	private long id;
	private long attribute_id;//所属属性ID
	private String option_value;//选项值
	private String option_name;//选项名称
	private String option_img;//选项图
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getAttribute_id() {
		return attribute_id;
	}
	public void setAttribute_id(long attribute_id) {
		this.attribute_id = attribute_id;
	}
	public String getOption_value() {
		return option_value;
	}
	public void setOption_value(String option_value) {
		this.option_value = option_value;
	}
	public String getOption_name() {
		return option_name;
	}
	public void setOption_name(String option_name) {
		this.option_name = option_name;
	}
	public String getOption_img() {
		return option_img;
	}
	public void setOption_img(String option_img) {
		this.option_img = option_img;
	}

}
