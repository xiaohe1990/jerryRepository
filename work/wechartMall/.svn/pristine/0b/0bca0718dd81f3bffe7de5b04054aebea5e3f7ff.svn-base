public class GShopcar {
	private long id;
	private long uid;//所属用户ID
	private long g_sku_id;//所属商品属性组合中间表ID
	private long org_id;//所属商家机构ID
	private int status;//1正常0禁用 -1删除
	private String add_time;//添加时间
	private String up_time;//修改时间
	private String u_ip;//用户IP
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getUid() {
		return uid;
	}
	public void setUid(long uid) {
		this.uid = uid;
	}
	public long getG_sku_id() {
		return g_sku_id;
	}
	public void setG_sku_id(long g_sku_id) {
		this.g_sku_id = g_sku_id;
	}
	public long getOrg_id() {
		return org_id;
	}
	public void setOrg_id(long org_id) {
		this.org_id = org_id;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getAdd_time() {
		return add_time;
	}
	public void setAdd_time(String add_time) {
		this.add_time = add_time;
	}
	public String getUp_time() {
		return up_time;
	}
	public void setUp_time(String up_time) {
		this.up_time = up_time;
	}
	public String getU_ip() {
		return u_ip;
	}
	public void setU_ip(String u_ip) {
		this.u_ip = u_ip;
	}

}
